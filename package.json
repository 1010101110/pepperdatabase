{
  "scripts": {
    "build:client": "vite build --config ./vite.config.js --outDir dist/client",
    "build:server": "vite build --config ./vite.config.js --ssr client/entry-server.js --outDir dist/server",
    "build": "npm run build:client && npm run build:server",
    "start": "node server/server.js",
    "restart": "npm run build:client && npm run build:server && node server/server.js"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/formbody": "^8.0.2",
    "@fastify/mysql": "^5.0.2",
    "@fastify/static": "^8.2.0",
    "@mdi/font": "^7.4.47",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/server-renderer": "^3.5.16",
    "fastify": "^5.3.3",
    "vite": "^6.3.5",
    "vite-plugin-vuetify": "^2.1.1",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1",
    "vuetify": "^3.8.8"
  }
}
